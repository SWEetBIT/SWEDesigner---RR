\subsubsection{UC6.3 - Mainwindows} 
\label{sssec:UC6.3} 
\begin{itemize} 
\item \textbf{Attori}: .
\item \textbf{Descrizione}: Mainwindows;
\item \textbf{Precondizione}: Mainwindows;
\item \textbf{Postcondizione}: Mainwindows;
\item \textbf{Scenario principale}: \begin{enumerate}\item MainFrame (UC6.3.1);\item Zoom-in (UC6.3.2);\item Zoom-out (UC6.3.3);\item Drag di un oggetto (UC6.3.4);\item Aggiunta relazione (UC6.3.5);\item Bottoni Switch (UC6.3.6);\item SideFrame (UC6.3.7). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1 - MainFrame} 
\label{sssec:UC6.3.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Interazione con la schermata principale;
\item \textbf{Precondizione}: L'utente crea o apre un progetto.;
\item \textbf{Postcondizione}: L'utente chiude il progetto.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Elimina Classe (UC6.3.1.1);\item Modifica interfaccia (UC6.3.1.10);\item Elimina Relazione (UC6.3.1.2);\item Elimina pacchetto (UC6.3.1.3);\item Elimina Flowchart (UC6.3.1.4);\item Modifica Classe (UC6.3.1.5);\item Modifica Relazione (UC6.3.1.6);\item Modifica pacchetto (UC6.3.1.7);\item Modifica flowchart (UC6.3.1.8);\item Eliminazione Interfaccia (UC6.3.1.9). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.1 - Elimina Classe} 
\label{sssec:UC6.3.1.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Una classe viene eliminata dal progetto corrente;
\item \textbf{Precondizione}: Deve essere stata disegnata una classe;
\item \textbf{Postcondizione}: Viene eliminata la classe selezionata e, a cascata, tutti i metodi (quindi i flowchart) ad essa, e solo ad essa, associati, tutti gli attributi e tutte le relazioni che la coinvolgono.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Elimina Nome (UC6.3.1.1.1);\item Elimina Attributo (UC6.3.1.1.2);\item Elimina Metodo (UC6.3.1.1.3). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.1.1 - Elimina Nome} 
\label{sssec:UC6.3.1.1.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione Nome di una classe;
\item \textbf{Precondizione}: È stata creata una classe con un nome.;
\item \textbf{Postcondizione}: Viene eliminato il nome della classe.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe fornendole un nome e desidera cancellarlo.;\end{itemize} 
\subsubsection{UC6.3.1.1.2 - Elimina Attributo} 
\label{sssec:UC6.3.1.1.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene eliminato un attributo della classe.;
\item \textbf{Precondizione}: È stata creata una classe con almeno un attributo.;
\item \textbf{Postcondizione}: L'attributo selezionato viene eliminato.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe con almeno un attributo al suo interno e desidera eliminarlo.;\item \textbf{Scenari alternativi}: L'utente ha aggiunto un nuovo attributo e desidera eliminarlo..
\end{itemize} 
\subsubsection{UC6.3.1.1.3 - Elimina Metodo} 
\label{sssec:UC6.3.1.1.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene eliminato un metodo della classe.;
\item \textbf{Precondizione}: È stata creata una classe con almeno un metodo.;
\item \textbf{Postcondizione}: Viene eliminato il metodo selezionato e, a cascata, il flowchart associato se esistente.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe con almeno un metodo ed  eventualmente anche il flowchart corrispondente e desidera eliminarlo.;\item \textbf{Scenari alternativi}: L'utente ha aggiunto un nuovo metodo e desidera eliminarlo..
\end{itemize} 
\subsubsection{UC6.3.1.10 - Modifica interfaccia} 
\label{sssec:UC6.3.1.10} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di un'interfaccia presente sul designer.;
\item \textbf{Precondizione}: § stata creata un'interfaccia.;
\item \textbf{Postcondizione}: L'interfaccia è stata modificata in uno o più campi dato.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Modifica Nome (UC6.3.1.10.1);\item Modifica attributo (UC6.3.1.10.2);\item Modifica Metodo (UC6.3.1.10.3). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.10.1 - Modifica Nome} 
\label{sssec:UC6.3.1.10.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica del nome dell'interfaccia.;
\item \textbf{Precondizione}: È stata creata un'interfaccia sul designer.;
\item \textbf{Postcondizione}: Il nome dell'interfaccia è stato modificato.;
\item \textbf{Scenario principale}: L'utente ha disegnato un'interfaccia e desidera modificarne il nome.;\end{itemize} 
\subsubsection{UC6.3.1.10.2 - Modifica attributo} 
\label{sssec:UC6.3.1.10.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di un attributo dell'interfaccia.;
\item \textbf{Precondizione}: È stata creata un'interfaccia con almeno un attributo al suo interno.;
\item \textbf{Postcondizione}: L'attributo selezionato è stato modificato.;
\item \textbf{Scenario principale}: L'utente ha disegnato un'interfaccia con almeno un attributo al suo interno e desidera modificarlo.;\end{itemize} 
\subsubsection{UC6.3.1.10.3 - Modifica Metodo} 
\label{sssec:UC6.3.1.10.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di un metodo dell'interfaccia.;
\item \textbf{Precondizione}: È stata creata un'interfaccia con almeno un metodo al suo interno.;
\item \textbf{Postcondizione}: Il metodo selezionato è stato modificato.;
\item \textbf{Scenario principale}: L'utente ha creato un'interfaccia con almeno un metodo al suo interno e desidera modificarlo.;\end{itemize} 
\subsubsection{UC6.3.1.2 - Elimina Relazione} 
\label{sssec:UC6.3.1.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene eliminata una relazione fra oggetti all'interno del designer.;
\item \textbf{Precondizione}: È stata disegnata una Relazione.;
\item \textbf{Postcondizione}: Viene eliminata la relazione selezionata ed eventuali etichette associate.;
\item \textbf{Scenario principale}: L'utente ha disegnato una relazione, la seleziona e decide di cancellarla.;\end{itemize} 
\subsubsection{UC6.3.1.3 - Elimina pacchetto} 
\label{sssec:UC6.3.1.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di un package;
\item \textbf{Precondizione}: È stato disegnato un pacchetto.;
\item \textbf{Postcondizione}: Viene eliminato il pacchetto dal designer assieme a tutti i suoi eventuali elementi contenuti.;
\item \textbf{Scenario principale}: L'utente ha disegnato un pacchetto e lo ha riempito con delle classi prima di cancellarlo.;\end{itemize} 
\subsubsection{UC6.3.1.4 - Elimina Flowchart} 
\label{sssec:UC6.3.1.4} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Elimina il flowchart di un metodo.;
\item \textbf{Precondizione}: È stato creato il flowchart di un metodo.;
\item \textbf{Postcondizione}: Elimina il flowchart di un metodo, i collegamenti con le classi coinvolte e tutti i suoi elementi.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Elimina punto entrata/uscita (UC6.3.1.4.1);\item Eliminazione blocco I/O (UC6.3.1.4.2);\item Eliminazione relazione (UC6.3.1.4.3);\item Eliminazione processo (UC6.3.1.4.4);\item Eliminazione blocco decisione (UC6.3.1.4.5);\item Eliminazione processo predefinito (UC6.3.1.4.6);\item Eliminare una preparazione (UC6.3.1.4.7);\item Eliminare on-page connector (UC6.3.1.4.8);\item Eliminare off-page connector (UC6.3.1.4.9). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.4.1 - Elimina punto entrata/uscita} 
\label{sssec:UC6.3.1.4.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione del punto di entrata o di uscita del flowchart.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un punto di entrata o almeno un punto di uscita.;
\item \textbf{Postcondizione}: Il flowchart risultante manca di entrata o uscita e non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha creato un flowchart con almeno un unto di entrata o di uscita che desidera eliminare.;\end{itemize} 
\subsubsection{UC6.3.1.4.2 - Eliminazione blocco I/O} 
\label{sssec:UC6.3.1.4.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione del blocco I/O selezionato.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un blocco di I/o al suo interno.;
\item \textbf{Postcondizione}: Il blocco di I/O selezionato viene eliminato assieme alle sue relazioni. Il flowchart risultante non può generare codice.;
\item \textbf{Scenario principale}: L'utente ha creato un flowchart con almeno un blocco di I/O che vuole eliminare.
;\end{itemize} 
\subsubsection{UC6.3.1.4.3 - Eliminazione relazione} 
\label{sssec:UC6.3.1.4.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione della relazione fra i blocchi del flowchart.;
\item \textbf{Precondizione}: Esiste un flowchart contenente almeno due elementi in relazione fra loro.;
\item \textbf{Postcondizione}: La relazione fra i due elementi viene cancellata assieme alla sua annotazione e il flowchart risultante non può generare codice.;
\item \textbf{Scenario principale}: L'utente ha creato un flowchart con almeno due elementi in connessione fra loro e desidera eliminare quest'ultima.;\end{itemize} 
\subsubsection{UC6.3.1.4.4 - Eliminazione processo} 
\label{sssec:UC6.3.1.4.4} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione del blocco di processo.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un blocco di processo al suo interno.;
\item \textbf{Postcondizione}: Viene eliminato il blocco di processo selezionato assieme alle sue relazioni. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno un blocco di rpocesso al suo interno e desidera eliminarlo.;\end{itemize} 
\subsubsection{UC6.3.1.4.5 - Eliminazione blocco decisione} 
\label{sssec:UC6.3.1.4.5} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di un blocco di decisione all'interno del flowchart.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un blocco di decisione al suo interno.;
\item \textbf{Postcondizione}: Il blocco di decisione selezionato è stato eliminato assieme alle sue relazioni. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha  disegnato un flowchart con almeno un blocco di decisione al suo interno e desidera eliminarlo.;\end{itemize} 
\subsubsection{UC6.3.1.4.6 - Eliminazione processo predefinito} 
\label{sssec:UC6.3.1.4.6} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di un processo predefinito all'interno del flowchart.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un processo predefinito al suo interno.;
\item \textbf{Postcondizione}: Il processo predefinito selezionato viene eliminato e con lui le sue relazioni e il flowchart di riferimento. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno un processo predefinito al suo interno e desidera eliminarlo.;\end{itemize} 
\subsubsection{UC6.3.1.4.7 - Eliminare una preparazione} 
\label{sssec:UC6.3.1.4.7} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di una preparazione;
\item \textbf{Precondizione}: È stato disegnato un flowchart con almeno una preparazione al suo interno.;
\item \textbf{Postcondizione}: La preparazione selezionata viene eliminata assieme alle sue relazioni. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno una preparazione al suo interno e desidera eliminarla.;\end{itemize} 
\subsubsection{UC6.3.1.4.8 - Eliminare on-page connector} 
\label{sssec:UC6.3.1.4.8} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di un on-page connector;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un on-page connector al suo interno.;
\item \textbf{Postcondizione}: L'on-page connector selezionato viene eliminato assieme alle sue relazioni. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno un on-page connector al suo interno e desidera eliminarlo.;\end{itemize} 
\subsubsection{UC6.3.1.4.9 - Eliminare off-page connector} 
\label{sssec:UC6.3.1.4.9} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Eliminazione di un off-page connector dal flowchart.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un off-page connector al suo interno.;
\item \textbf{Postcondizione}: L'off-page connector viene eliminato assieme alle sue relazioni. Il flowchart risultante non potrà generare codice.;
\item \textbf{Scenario principale}: L'utente ha creato un flowchart con almeno un off-page connector e desidera eliminarlo.
;\end{itemize} 
\subsubsection{UC6.3.1.5 - Modifica Classe} 
\label{sssec:UC6.3.1.5} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica degli elementi interni ad una classe;
\item \textbf{Precondizione}: È stata disegnata una classe sul progetto.;
\item \textbf{Postcondizione}: La classe risulta modificata.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Modifica Nome (UC6.3.1.5.1);\item Modifica Attributo (UC6.3.1.5.2);\item Modifica Metodo (UC6.3.1.5.3). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.5.1 - Modifica Nome} 
\label{sssec:UC6.3.1.5.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene modificato il nome della classe.;
\item \textbf{Precondizione}: È stata creata una classe con un nome.;
\item \textbf{Postcondizione}: Viene modificato il nome della classe.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe con un nome e desidera modificarlo.;\end{itemize} 
\subsubsection{UC6.3.1.5.2 - Modifica Attributo} 
\label{sssec:UC6.3.1.5.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene modificato l'attributo della classe.;
\item \textbf{Precondizione}: È stata disegnata una classe con almeno un attributo.;
\item \textbf{Postcondizione}: Viene modificato l'attributo selezionato.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe con almeno un attributo al suo interno e desidera modificarlo.;\end{itemize} 
\subsubsection{UC6.3.1.5.3 - Modifica Metodo} 
\label{sssec:UC6.3.1.5.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene modificato il metodo della classe.;
\item \textbf{Precondizione}: È stata creata una classe con almeno un metodo al suo interno.;
\item \textbf{Postcondizione}: Il metodo selezionato viene modificato.;
\item \textbf{Scenario principale}: L'utente ha disegnato una classe con almeno un metodo al suo interno.;\end{itemize} 
\subsubsection{UC6.3.1.6 - Modifica Relazione} 
\label{sssec:UC6.3.1.6} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene modificata l'etichetta della relazione.;
\item \textbf{Precondizione}: È stata creata una relazione fra due o più classi.;
\item \textbf{Postcondizione}: L'etichetta della relazione corrente è stata modificata.;
\item \textbf{Scenario principale}: L'utente ha creato una nuova relazione fra classi e desidera modificarne l'etichetta.;\end{itemize} 
\subsubsection{UC6.3.1.7 - Modifica pacchetto} 
\label{sssec:UC6.3.1.7} 
\begin{itemize} 
\item \textbf{Attori}: .
\item \textbf{Descrizione}: Modifica un pacchetto.;
\item \textbf{Precondizione}: È stato creato un pacchetto.;
\item \textbf{Postcondizione}: Il pacchetto è stato modificato.;
\item \textbf{Scenario principale}: \begin{enumerate}\item Modifica Nome (UC6.3.1.7.1);\item Eliminare classe (UC6.3.1.7.2);\item Estrarre classe (UC6.3.1.7.3). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.7.1 - Modifica Nome} 
\label{sssec:UC6.3.1.7.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica il nome di un pacchetto.;
\item \textbf{Precondizione}: È stato creato un pacchetto.;
\item \textbf{Postcondizione}: Il nome del pacchetto selezionato viene modificato.;
\item \textbf{Scenario principale}: L'utente ha creato un nuovo pacchetto di cui desidera cambiare il nome.;\end{itemize} 
\subsubsection{UC6.3.1.7.2 - Eliminare classe} 
\label{sssec:UC6.3.1.7.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Viene eliminata una classe all'interno del pacchetto.;
\item \textbf{Precondizione}: Esiste un pacchetto con almeno una classe al suo interno.;
\item \textbf{Postcondizione}: La classe selezionata viene eliminata dal pacchetto.;
\item \textbf{Scenario principale}: L'utente ha inserito almeno una classe all'interno del pacchetto.;\end{itemize} 
\subsubsection{UC6.3.1.7.3 - Estrarre classe} 
\label{sssec:UC6.3.1.7.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Una classe viene estratta dal pacchetto.;
\item \textbf{Precondizione}: Esiste un pacchetto con almeno una classe al suo interno.;
\item \textbf{Postcondizione}: La classe selezionata si trova fuori dal pacchetto.;
\item \textbf{Scenario principale}: L'utente trascina la classe dal pacchetto al suo esterno.;\end{itemize} 
\subsubsection{UC6.3.1.8 - Modifica flowchart} 
\label{sssec:UC6.3.1.8} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di un flowchart selezionato;
\item \textbf{Precondizione}: È stato creato un flowchart;
\item \textbf{Postcondizione}: Il flowchart selezionato è stato modificato;
\item \textbf{Scenario principale}: \begin{enumerate}\item Modifica blocchi inizio/fine (UC6.3.1.8.1);\item Modifica blocco di I/O (UC6.3.1.8.2);\item Modifica relazione (UC6.3.1.8.3);\item Modifica processo (UC6.3.1.8.4);\item Modifica processo predefinito (UC6.3.1.8.5);\item Modifica di una preparazione (UC6.3.1.8.6);\item Modifica on-page connector (UC6.3.1.8.7);\item Modifica off-page connector (UC6.3.1.8.8). 
 \end{enumerate}
\end{itemize} 
\subsubsection{UC6.3.1.8.1 - Modifica blocchi inizio/fine} 
\label{sssec:UC6.3.1.8.1} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica dei blocchi di inizio/fine;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un blocco di inizio/fine.;
\item \textbf{Postcondizione}: I blocchi di inizio/fine vengono modificati dall'utente.;
\item \textbf{Scenario principale}: L'utente ha creato un flowchart con almeno un blocco di inizio/fine;\end{itemize} 
\subsubsection{UC6.3.1.8.2 - Modifica blocco di I/O} 
\label{sssec:UC6.3.1.8.2} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di un blocco I/O all'interno del flowchart.;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno un blocco di I/O al suo interno.;
\item \textbf{Postcondizione}: Il blocco di I/O selezionato viene modificato riferendosi così ad un altro elemento di una classe.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno un blocco di I/O al suo interno e desidera modificarlo.;\end{itemize} 
\subsubsection{UC6.3.1.8.3 - Modifica relazione} 
\label{sssec:UC6.3.1.8.3} 
\begin{itemize} 
\item \textbf{Attori}: Utente Autenticato.
\item \textbf{Descrizione}: Modifica di una relazione all'interno del flowchart;
\item \textbf{Precondizione}: È stato creato un flowchart con almeno due elementi in relazione fra di loro.;
\item \textbf{Postcondizione}: L'annotazione della relazione fra i due oggetti è stata modificata.;
\item \textbf{Scenario principale}: L'utente ha disegnato un flowchart con almeno due elementi in realzione fra di loro e desidera modificare quest'ultima.;\end{itemize} 
\subsubsection{UC6.3.1.8.4 - Modifica processo} 
\label{sssec:UC6.3.1.8.4} 
